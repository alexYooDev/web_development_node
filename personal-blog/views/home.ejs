<%- include('partials/header') %>
<h1>Home</h1>
<p><%=textContent %></p>
  <% posts.forEach(function(post){ %>
    <div>
      <h2><%= post.title%></h2>
      <p>
        <%= post.post.substring(0, 100)%> ... 
        <a href="posts/<%=post._id%>">Read More</a>
      </p>
    </div>
  <% }); %>
<%- include('partials/footer') %>